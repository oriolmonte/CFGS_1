<Application x:Class="Temes.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Temes"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Tema\TabControl.xaml"/>
                <ResourceDictionary Source="Tema\TabItem.xaml"/>
                <ResourceDictionary Source="Tema\Button.xaml"/>
                <ResourceDictionary Source="Tema\GroupBox.xaml"/>
                <ResourceDictionary Source="Tema\TextBox.xaml"/>
                <ResourceDictionary Source="Tema\ComboBox.xaml"/>
                <ResourceDictionary Source="Tema\TreeView.xaml"/>
                <ResourceDictionary Source="Tema\Menu.xaml"/>
                <ResourceDictionary Source="Tema\ProgressBar.xaml"/>
                <ResourceDictionary Source="Tema\ToolTip.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
